{
  "name": "village-dummy-data",
  "version": "0.1.0",
  "description": "A DataSupplier plugin for commonly used Village dummy data.",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Village Dummy Data",
    "manifest": "manifest.json",
    "main": "village-dummy-data.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4"
  },
  "author": "eaugustine <eaugustine@gmail.com>",
  "dependencies": {
    "lodash": "^4.17.21",
    "sketch": "^0.2.0"
  }
}
